@if (ViewData["Staff"] != null)
{
    var staff = ViewData["Staff"] as GripsStore.Models.Staff;


    <form>
        <div class="form-group">
            <label for="staffcode">staffCode</label>
            <input type="text" class="form-control" id="staffcode" value="@staff.staffCode">
        </div>
        <div class="form-group">
            <label for="kanjiName">kanjiName</label>
            <input type="text" class="form-control" id="kanjiName" value="@staff.kanjiName">
        </div>
        <div class="form-group">
            <label for="kanaName">kanaName</label>
            <input type="text" class="form-control" id="kanaName" value="@staff.kanaName">
        </div>
        @*<div>
                <button type="submit" class="btn btn-default">Update</button>
            </div>*@
        <div id="admin" class="row">
            <button id="btn-cancel" class="btn btn-danger">Cancel</button>
            <button id="btn-update" class="btn btn-primary">Update</button>
        </div>
    </form>



}
